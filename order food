#include<stdio.h>
main()
{
	printf("Welcome to zomotato:\n");
	printf("\nchoose what you want:\n1. Pizza, Rs 239. \n2. Burger, Rs 129. \n3. Pasta, Rs 179. \n4. French Fries, Rs 99. \n5. Sandwich, Rs 149.");
	int choice=0;
	scanf("%d",&choice);
	
	switch(choice)
	{
		case 1:
			printf("Food iteam - Pizza. \nPrice - Rs 239.");
			break;
		case 2:
			printf("Food iteam - Burger. \nPrice - Rs 129.");
			break;
		case 3:
			printf("Food iteam - Pasta. \nPrice - Rs 179.");
			break;
		case 4:
			printf("Food iteam - French Fries. \nPrice - Rs 99.");
			break;
		case 5:
			printf("Food iteam - Sandwich. \nPrice - Rs 149.");
			break;
		deafault : printf("Invalid choice");
	}
}
